{
  "hosting": [
    {
      "site": "my-grid-layout",
      "public": "dist/grid-layout",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
